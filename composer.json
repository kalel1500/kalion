{
    "name": "kalel1500/kalion",
    "description": "Utilidades para desarrollar en arquitectura hexagonal y DDD en laravel",
    "keywords": ["kalion","laravel", "package", "hexagonal", "architecture", "ddd", "utilities"],
    "authors": [
        {
            "name": "Adrian Canals Csefko",
            "email": "a.canals14@gmail.com"
        }
    ],
    "license": "GPL-3.0-or-later",
    "require": {
        "ext-json": "*",
        "php" : "^8.2",
        "laravel/framework": "^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "orchestra/testbench": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Thehouseofel\\Kalion\\": "src",
            "Thehouseofel\\Kalion\\Database\\Factories\\": "database/factories",
            "Thehouseofel\\Kalion\\Database\\Seeders\\": "database/seeders"
        },
        "files": [
            "src/Core/Domain/Helpers/helpers_domain.php",
            "src/Core/Infrastructure/Helpers/helpers_infrastructure.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Thehouseofel\\Kalion\\Tests\\": "tests/",
            "Thehouseofel\\Kalion\\Tests\\Support\\": "tests/Support/",
            "Thehouseofel\\Kalion\\Tests\\Support\\Database\\Factories\\": "tests/Support/database/factories",
            "Thehouseofel\\Kalion\\Tests\\Support\\Database\\Seeders\\": "tests/Support/database/seeders"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thehouseofel\\Kalion\\Core\\Infrastructure\\KalionServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
